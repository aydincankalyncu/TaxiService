@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Innsbruck Taxi | Airport</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="~/images/logo.png" type="image/x-icon" />

    <link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="~/css/main/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="~/css/main/css/animate.css">

    <link rel="stylesheet" href="~/css/main/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/main/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/css/main/css/magnific-popup.css">

    <link rel="stylesheet" href="~/css/main/css/aos.css">

    <link rel="stylesheet" href="~/css/main/css/ionicons.min.css">

    <link rel="stylesheet" href="~/css/main/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/css/main/css/jquery.timepicker.css">


    <link rel="stylesheet" href="~/css/main/css/flaticon.css">
    <link href="~/css/icofont/icofont.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/main/css/icomoon.css">
    <link rel="stylesheet" href="~/css/main/css/style.css">
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index","Home")">@localizer["InnsbruckLabel"]<span>@localizer["Taxi"]</span></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> @localizer["Menu"]
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active"><a href="@Url.Action("Index","Home")" class="nav-link">@localizer["Home"]</a></li>
                    <li class="nav-item"><a href="@Url.Action("List","About")" class="nav-link">@localizer["About"]</a></li>
                    <li class="nav-item"><a href="@Url.Action("List","Resort")" class="nav-link">@localizer["SkiResorts"]</a></li>
                    <li class="nav-item"><a href="@Url.Action("BookingService","Reservation")" class="nav-link">@localizer["Booking"]</a></li>
                    <li class="nav-item"><a href="@Url.Action("Info","Contact")" class="nav-link">@localizer["Contact"]</a></li>
                </ul>
                <partial name="_Culture" />
            </div>
        </div>
    </nav>
    <!-- END nav -->

    @RenderBody()

    <footer class="ftco-footer ftco-bg-dark ftco-section">
        <div class="container">
            <div class="row mb-5">
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">@localizer["AboutTaxi"]</h2>
                        <p>@localizer["AboutLabel"]</p>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                            <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                            <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                            <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4 ml-md-5">
                        <h2 class="ftco-heading-2">@localizer["Info"]</h2>
                        <ul class="list-unstyled">
                            <li><a href="@Url.Action("List","About")" class="py-2 d-block">@localizer["About"]</a></li>
                            <li><a href="@Url.Action("Info","Contact")" class="py-2 d-block">@localizer["Contact"]</a></li>
                            <li><a href="@Url.Action("List","Resort")" class="py-2 d-block">@localizer["SkiResorts"]</a></li>
                            <li><a href="@Url.Action("Index","Home")" class="py-2 d-block">@localizer["Home"]</a></li>
                            <li><a href="@Url.Action("BookingService","Reservation")" class="py-2 d-block">@localizer["Booking"]</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">@localizer["HaveQuestion"]</h2>
                        <div class="block-23 mb-3">
                            <ul>
                                <li>
                                    <span class="icon icon-map-marker"></span><span class="text">
                                        Innsbruck - Wetterhenweg
                                    </span>
                                </li>
                                <li><a><span class="icon icon-phone"></span><span class="text">004366499398308</span></a></li>
                                <li><a href="@Url.Action("Info","Contact")"><span class="icon icon-envelope"></span><span class="text">infoinnsbrucktaxi@gmail.com</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <p>
                        @localizer["Copyright"] &copy;
                        <script>document.write(new Date().getFullYear());</script> @localizer["AllRights"]
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <p style="font-size: 0.7em;">KALYONCU SOFTWARE <span>aydincankalyncu@gmail.com</span></p>
                </div>
            </div>
        </div>
    </footer>



    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

    <script src="~/js/Kontroller.js" type="text/javascript"></script>
    <script src="~/js/BookingEvents.js"></script>
    <script src="~/js/main/js/jquery.min.js"></script>
    <script src="~/js/main/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/js/main/js/popper.min.js"></script>
    <script src="~/js/main/js/bootstrap.min.js"></script>
    <script src="~/js/main/js/jquery.easing.1.3.js"></script>
    <script src="~/js/main/js/jquery.waypoints.min.js"></script>
    <script src="~/js/main/js/jquery.stellar.min.js"></script>
    <script src="~/js/main/js/owl.carousel.min.js"></script>
    <script src="~/js/main/js/jquery.magnific-popup.min.js"></script>
    <script src="~/js/main/js/aos.js"></script>
    <script src="~/js/main/js/jquery.animateNumber.min.js"></script>
    <script src="~/js/main/js/bootstrap-datepicker.js"></script>
    <script src="~/js/main/js/jquery.timepicker.min.js"></script>
    <script src="~/js/main/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="~/js/main/js/google-map.js"></script>
    <script src="~/js/main/js/main.js"></script>
    <script src="~/js/main/js/bootstrap-datepicker.de.js"></script>
    <script src="~/js/main/js/bootstrap-datepicker.en-GB.js"></script>
</body>
</html>